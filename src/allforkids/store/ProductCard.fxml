<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxWidth="260.0" minWidth="260.0" onKeyPressed="#goToProduct" prefWidth="260.0" stylesheets="@store.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="allforkids.store.ProductCardController">
   <children>
      <VBox maxWidth="302.0" minWidth="302.0" prefWidth="302.0" styleClass="post" stylesheets="@../blog/blog.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
         <children>
            <AnchorPane fx:id="imageContainer" disable="true" maxHeight="200.0" maxWidth="228.0" minHeight="200.0" minWidth="228.0" pickOnBounds="false" prefHeight="200.0" prefWidth="228.0" VBox.vgrow="NEVER" />
            <AnchorPane>
               <children>
                  <BorderPane AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <right>
                        <JFXButton fx:id="productPrice" onAction="#goToProduct" style="-fx-background-color: #f37d8a; -fx-background-radius: 15;" text="Price" textFill="WHITE" BorderPane.alignment="CENTER" />
                     </right>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <left>
                        <JFXButton fx:id="productName" onAction="#goToProduct" ripplerFill="TRANSPARENT" text="Button" textOverrun="CLIP" BorderPane.alignment="CENTER_LEFT" />
                     </left>
                  </BorderPane>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
