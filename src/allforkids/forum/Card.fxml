<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="80.0" prefWidth="1000.0" styleClass="card" stylesheets="@forum.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="allforkids.forum.CardController">
   <children>
      <VBox prefWidth="900.0">
         <children>
            <AnchorPane prefHeight="40.0">
               <children>
                  <Label fx:id="titleLabel" styleClass="card-title" text="topicNameLabel">
                     <font>
                        <Font size="15.0" />
                     </font></Label>
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="40.0">
               <children>
                  <Label fx:id="subTitleLabel" styleClass="card-subtitle" text="subTitleLabel" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
      <JFXButton onAction="#onClick" prefHeight="80.0" prefWidth="79.0" styleClass="primary-btn" text="&gt;">
         <font>
            <Font size="24.0" />
         </font></JFXButton>
   </children>
</HBox>
